{
    "name": "wdelfuego/nova-calendar",
    "description": "A minimalistic event calendar Tool for Laravel's Nova 4.",
    "keywords": [
        "laravel",
        "nova",
        "calendar",
        "event-calendar",
        "events",
        "date"
    ],
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "laravel/nova": "~4.0",
        "wdelfuego/nova-datetime": "^1.0.1"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wdelfuego\\NovaCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wdelfuego\\NovaCalendar\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
